{
  "_meta": {
    "version": "2.0",
    "notes": "UG017 logs with ASCII-style blocks; indices start after standard header.",
    "split": {
      "data_sep": ",",
      "checksum_sep": "*"
    },
    "converters": {
      "lat_dm": "ddmm.mmmm → deg",
      "lon_dm": "dddmm.mmmm → deg",
      "time_hms": "hhmmss[.ss]"
    }
  },
  "ATR": {
    "label": "Navigation information like positioning and heading",
    "description": "Navigation information (position + heading)",
    "ascii": {
      "tag": "ATR",
      "log_command": "gpatr",
      "description": "Navigation information like positioning and heading",
      "default_rate_hz": 1,
      "aliases": [
        "$GPATR",
        "ATR"
      ],
      "fields": [
        {
          "index": 1,
          "name": "time_utc",
          "type": "time_hms",
          "unit": "UTC"
        },
        {
          "index": 2,
          "name": "pos_status",
          "type": "int",
          "unit": "",
          "note": "Positioning status (a0); see note: 0=no solution, 1=single point, 2=pseudorange diff, 4=fixed, 5=float."
        },
        {
          "index": 3,
          "name": "baseline_len_m",
          "type": "float",
          "unit": "m",
          "note": "Baseline length (x1)."
        },
        {
          "index": 4,
          "name": "north_m",
          "type": "float",
          "unit": "m",
          "note": "North distance (x2)."
        },
        {
          "index": 5,
          "name": "east_m",
          "type": "float",
          "unit": "m",
          "note": "East distance (x3)."
        },
        {
          "index": 6,
          "name": "up_m",
          "type": "float",
          "unit": "m",
          "note": "Up distance (x4)."
        },
        {
          "index": 7,
          "name": "head_status",
          "type": "int",
          "unit": "",
          "note": "Heading status (a1); same codes as positioning status (0/1/2/4/5)."
        },
        {
          "index": 8,
          "name": "yaw_deg",
          "type": "float",
          "unit": "deg",
          "note": "Yaw (x5)."
        },
        {
          "index": 9,
          "name": "pitch_deg",
          "type": "float",
          "unit": "deg",
          "note": "Pitch (x6)."
        },
        {
          "index": 10,
          "name": "reserved",
          "type": "str",
          "unit": ""
        },
        {
          "index": 11,
          "name": "checksum",
          "type": "str",
          "unit": "",
          "note": "NMEA checksum (hh)."
        }
      ],
      "notes": [
        "Position/heading status codes: 0=no solution; 1=single point; 2=pseudorange differential; 4=fixed; 5=float."
      ],
      "name": "Navigation information (position + heading)"
    }
  },
