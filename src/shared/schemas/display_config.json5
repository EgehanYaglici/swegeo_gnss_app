        {"name": "ins_status", "label": "INS Status", "unit": "", "format": "str"}
      ],

      "sources": {
        "CORRIMUDATAB": {
          "id": 812,
          "type": "binary",
          "description": "Corrected IMU Data - Incremental values per sample",
          "field_mapping": {
            "accel_x": "lateral_acc",        // X-axis (right)
            "accel_y": "longitudinal_acc",   // Y-axis (forward)
            "accel_z": "vertical_acc",       // Z-axis (up)
            "gyro_x": "roll_rate_rad",       // about Y-axis → X rotation
            "gyro_y": "pitch_rate_rad",      // about X-axis → Y rotation
            "gyro_z": "yaw_rate_rad"         // about Z-axis (same)
          },
          "extra_fields": [],
          "conversions": {
            "gyro_x": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58},
            "gyro_y": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58},
            "gyro_z": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58}
          }
        },

        "CORRIMUDATASB": {
          "id": 813,
          "type": "binary",
          "description": "Corrected IMU Data Short - Incremental values per sample",
          "field_mapping": {
            "accel_x": "lateral_acc",        // X-axis (right)
            "accel_y": "longitudinal_acc",   // Y-axis (forward)
            "accel_z": "vertical_acc",       // Z-axis (up)
            "gyro_x": "roll_rate_rad",       // about Y-axis → X rotation
            "gyro_y": "pitch_rate_rad",      // about X-axis → Y rotation
            "gyro_z": "yaw_rate_rad"         // about Z-axis (same)
          },
          "extra_fields": [],
          "conversions": {
            "gyro_x": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58},
            "gyro_y": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58},
            "gyro_z": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58}
          }
        },

        "RAWIMUB": {
          "id": 268,
          "type": "binary",
          "description": "Raw IMU Data",
          // Sensor frame: +X forward, +Y right, +Z up
          // Display frame: X=lateral(right), Y=longitudinal(forward), Z=vertical(up)
          // nyA/nyG = NEGATIVE Y-axis → negate to get +Y (right)
          "field_mapping": {
            "accel_x": "nyA",            // Display X (right)  ← sensor -Y → negate via conversion
            "accel_y": "xA",             // Display Y (forward) ← sensor X
            "accel_z": "zA",             // Display Z (up)      ← sensor Z
            "gyro_x": "nyG",             // Display X (right)  ← sensor -Y → negate via conversion
            "gyro_y": "xG",              // Display Y (forward) ← sensor X
            "gyro_z": "zG"               // Display Z (up)      ← sensor Z
          },
          "conversions": {
            // LSB → m/s² (Table 4-16: 7.48188018798828e-06), negate ny* fields
            "accel_x": {"from": "LSB", "to": "m/s²", "factor": -7.48188018798828e-06},
            "accel_y": {"from": "LSB", "to": "m/s²", "factor": 7.48188018798828e-06},
            "accel_z": {"from": "LSB", "to": "m/s²", "factor": 7.48188018798828e-06},
            // LSB → deg/s (Table 4-16: 1.52587890625e-05), negate ny* fields
            "gyro_x": {"from": "LSB", "to": "deg/s", "factor": -1.52587890625e-05},
            "gyro_y": {"from": "LSB", "to": "deg/s", "factor": 1.52587890625e-05},
            "gyro_z": {"from": "LSB", "to": "deg/s", "factor": 1.52587890625e-05}
          },
          "extra_fields": [
            {"field": "imu_status", "label": "Status", "format": "int"},
            {"field": "week", "label": "Week", "format": "int"},
            {"field": "tow_s", "label": "ToW", "format": "float", "unit": "s", "decimals": 3}
          ]
        },
        
        "RAWIMUA": {
          "tag": "RAWIMUA",
          "type": "ascii",
          "description": "Raw IMU Data (ASCII)",
          // Same axis mapping + conversions as RAWIMUB
          "field_mapping": {
            "accel_x": "nyA",
            "accel_y": "xA",
            "accel_z": "zA",
            "gyro_x": "nyG",
            "gyro_y": "xG",
            "gyro_z": "zG"
          },
          "conversions": {
            "accel_x": {"from": "LSB", "to": "m/s²", "factor": -7.48188018798828e-06},
            "accel_y": {"from": "LSB", "to": "m/s²", "factor": 7.48188018798828e-06},
            "accel_z": {"from": "LSB", "to": "m/s²", "factor": 7.48188018798828e-06},
            "gyro_x": {"from": "LSB", "to": "deg/s", "factor": -1.52587890625e-05},
            "gyro_y": {"from": "LSB", "to": "deg/s", "factor": 1.52587890625e-05},
            "gyro_z": {"from": "LSB", "to": "deg/s", "factor": 1.52587890625e-05}
          },
          "extra_fields": [
            {"field": "imu_status", "label": "Status", "format": "int"}
          ]
        },

        "CORRIMUDATAA": {
          "tag": "CORRIMUDATAA",
          "type": "ascii",
          "description": "Corrected IMU Data (ASCII) - Incremental values per sample",
          "field_mapping": {
            // NovAtel uses: lateral(X-right), longitudinal(Y-forward), vertical(Z-up)
            "accel_x": "lateral_acc",         // X-axis (right)
            "accel_y": "longitudinal_acc",    // Y-axis (forward)
            "accel_z": "vertical_acc",        // Z-axis (up)

            // NovAtel frame is aircraft standard: X=forward, Y=right
            // Our sensor: X=right, Y=forward (90° rotated from aircraft)
            // NovAtel roll_rate (about aircraft Y=right) maps to our X=right
            // NovAtel pitch_rate (about aircraft X=forward) maps to our Y=forward
            "gyro_x": "roll_rate_rad",        // about Y-axis → X rotation
            "gyro_y": "pitch_rate_rad",       // about X-axis → Y rotation
            "gyro_z": "yaw_rate_rad"          // about Z-axis (same)
          },
          "extra_fields": [
            {"field": "week", "label": "Week", "format": "int"},
            {"field": "seconds_of_week", "label": "ToW", "format": "float", "unit": "s", "decimals": 3}
          ],
          "conversions": {
            "gyro_x": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58},
            "gyro_y": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58},
            "gyro_z": {"from": "rad/sample", "to": "deg/s", "factor": 5729.58}
          }
        },

